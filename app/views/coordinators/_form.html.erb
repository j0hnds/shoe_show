<%= form_for @coordinator do |f| %>
<label for="coordinator_first_name">First Name:</label>
  <%= f.text_field :first_name %>
<label for="coordinator_last_name">Last Name:</label>
  <%= f.text_field :last_name %>
  <%= fields_for @contact_info do |contact_form| %>
<label for="contact_info_email">Email:</label>
    <%= contact_form.text_field :email %>
  <% end %>
  <%= fields_for @phone do | phone_form | %>
<label for="phone_phone_number">Phone Number:</label>
    <%= phone_form.text_field :phone_number %>
  <% end %>
  <%= f.submit "Create" %>
<% end %>
